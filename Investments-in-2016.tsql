/* Write your T-SQL query statement below */
SELECT ROUND(SUM(tiv_2016), 2) AS tiv_2016
FROM Insurance
WHERE tiv_2015 IN (
    SELECT tiv_2015
    FROM Insurance
    GROUP BY tiv_2015
    HAVING COUNT(*) > 1
)
AND pid IN (
    SELECT i1.pid
    FROM Insurance i1
    LEFT JOIN Insurance i2
        ON i1.lat = i2.lat AND i1.lon = i2.lon AND i1.pid <> i2.pid
    WHERE i2.pid IS NULL
);
